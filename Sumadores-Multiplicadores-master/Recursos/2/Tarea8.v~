  //: dip g4 (A) @(384,47) /sn:0 /w:[ 0 ] /st:0
  //: switch g3 (w4) @(725,155) /sn:0 /R:2 /w:[ 0 ] /st:0
  led g2 (.I(S));   //: @(480,241) /sn:0 /R:2 /w:[ 1 ] /type:3
  led g1 (.I(w3));   //: @(262,153) /sn:0 /R:1 /w:[ 0 ] /type:3
  //: dip g5 (B) @(548,45) /sn:0 /w:[ 0 ] /st:0
  CSA4b g0 (.A(A), .B(B), .Cin(w4), .Cout(w3), .S(S));   //: @(327, 112) /sz:(326, 83) /sn:0 /p:[ Ti0>1 Ti1>1 Ri0>1 Lo0<1 Bo0<0 ]

endmodule

module FullAdd(S, Cout, Cin, B, A);
//: interface  /sz:(83, 83) /bd:[ Ti0>B(63/83) Ti1>A(19/83) Ti2>A(19/83) Ti3>B(63/83) Ri0>Cin(42/83) Ri1>Cin(42/83) Lo0<Cout(40/83) Lo1<Cout(40/83) Bo0<S(43/83) Bo1<S(43/83) ]
